#include <iostream>
using namespace std;

float percentual_de_reprovados (float quantidade_de_alunos, float quantidade_de_alunos_aprovados)

{
	return 100-((quantidade_de_alunos_aprovados/quantidade_de_alunos)*100);
}



int main() /*Ele não pede mas eu quis fazer para testar */
{
	int a;
	int b;
	cin >> a;
	cin >> b;
	cout << "Percentual de Rreprovados " << percentual_de_reprovados (a, b) << "%" << endl; // Você chama a função aqui em cout
	return 0;
}
